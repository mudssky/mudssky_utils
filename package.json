{
  "name": "mudssky_utils",
  "version": "1.0.0",
  "main": "index.js",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "test": "cargo test --all-features",
    "docs:build": "cargo doc --no-deps --all-features",
    "test:doc": "cargo test --doc",
    "prepare": "husky",
    "format": "cargo fmt --all",
    "format:check": "cargo fmt --all -- --check",
    "lint": "cargo clippy --workspace --all-targets -- -D warnings",
    "lint:quick": "cargo clippy -- -D warnings",
    "publish:dryrun": "cargo publish --dry-run",
    "check:all": "pnpm format:check && pnpm lint && pnpm test && pnpm docs:build && pnpm test:doc "
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mudssky/mudssky_utils.git"
  },
